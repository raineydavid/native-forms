{
  "expo": {
    "name": "NativeForms",
    "slug": "native-forms",
    "privacy": "public",
    "description": "Build forms, surveys and polls for React Native apps.",
    "githubUrl": "https://github.com/venits/native-forms",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
